{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "FiveDraftEmailBodiesObject",
  "description": "Schema for an object with a drafts array of exactly five draft email bodies (strings).",
  "type": "object",
  "properties": {
    "drafts": {
      "type": "array",
      "minItems": 5,
      "maxItems": 5,
      "items": {
        "type": "string",
        "description": "The draft email body."
      }
    }
  },
  "required": ["drafts"]
}
